                            </div>
                        </div><!-- /.modal-content -->
                    </div><!-- /.modal-dialog -->
                </div>
            </div>
        </div>
    </div>

    <script src="~/Content/bootstrap-3.3.7-dist/js/jquery-2.0.3.min.js"></script>
    <script src="~/Content/bootstrap-3.3.7-dist/js/bootstrap.min.js"></script>
    <script src="~/Content/jquery.bsgrid-1.37/merged/bsgrid.all.min.js"></script>
    <script src="~/Content/jquery.bsgrid-1.37/js/lang/grid.zh-CN.min.js"></script>
    <script src="~/Content/layer/layer.js"></script>
    <script src="~/Content/jss/jquery.form.js"></script>@*异步自动提交*@
    <script src="~/Content/jss/customfunction.js"></script>
    <script>
        var tabRechargeall;
        var tabinreview;
        function reloadValideImage() {
            $("#ValidateCode").attr("src", "/LoginMain/validcode?ts=" + new Date().getTime());
            // document.getElementById('ValidateCode').src = '/Main/validcode?ts=' + new Date().getTime();
        }
     
        $(document).ready(function () {          
            $.post("/AuthenticationManagement/Authentication/SelctStatustype", function (jsonData) {
                selectLoadData("StatusID", jsonData);
            });
            $.post("/AuthenticationManagement/Authentication/SelctStatus", function (jsonData) {
                selectLoadData("UpStatusID", jsonData);
            });
            tabRechargeall = $.fn.bsgrid.init('tabRechargeall', {
                url: '/AuthenticationManagement/Authentication/selsectEducationaback',
                autoLoad: true,//自动加载数据
                stripeRows: true,//隔行变色
                rowHoverColor: true,//划过行变色
                rowSelectedColor: true,//是否显示选中行背景色
                pageSize: 10,//默认页面大小
                pageSizeSelect: true,//是否选择分页页数下拉框
                pagingLittleToolbar: true,//精简的图标按钮分页工具条
                pagingToolbarAlign: "left"//分页工具条的显示位置              
            });          
            tabNoticeSearch();

            $("#selectqueren").hide();
            $("#Baseinforid").hide();
        });
        
        //权限访问
        $("#containerid").mouseenter(function () {
            $.post("/LoginMain/MoKuaiQuanXianOpTypeSelect", function (Data) {
                for (var i = 0; i < Data.length; i++) {
                    if (Data[i].Name == "学历认证" && Data[i].Optype == "查询") {
                        $("#selectqueren").show();
                        $("#Baseinforid").show();
                    }
                    if (Data[i].Name == "学历认证" && Data[i].Optype == "审核") {
                        $(".strShenHe").removeClass("hidden");

                    }
                    if (Data[i].Name == "学历认证" && Data[i].Optype == "修改") {

                        $(".strUpdata").removeClass("hidden");

                    }
                }
            });
        });
        function binreview() {
            document.getElementById("rule").style.visibility = "visible"
            $.post("/Fundsmanagement/management/SelctType", function (jsonData) {
                selectLoadData("selecttwo", jsonData);
            });
            if (tabinreview == null) {
                tabinreview = $.fn.bsgrid.init('tabinreview', {
                    url: '/AuthenticationManagement/Authentication/selsectrenzhengJilu',
                    autoLoad: true,//自动加载数据
                    stripeRows: true,//隔行变色
                    rowHoverColor: true,//划过行变色
                    rowSelectedColor: true,//是否显示选中行背景色
                    pageSize: 10,//默认页面大小
                    pageSizeSelect: true,//是否选择分页页数下拉框
                    pagingLittleToolbar: true,//精简的图标按钮分页工具条
                    pagingToolbarAlign: "left"//分页工具条的显示位置
                });              
            }
            else {
                tabinreview = tabinreview;
            }
            $(".bsgridPagingOutTab").css("min-width", "98%");
            tabinreviewSearch();
        }
      
        //认证记录多条件查询
        function tabinreviewSearch() {
            var SNserName = $("#SNserName").val();
            var InstitutionsID = $("#InstitutionsID").val();
            var EducationalBackgroundID = $("#EducationalBackgroundID").val();
            tabinreview.search("SNserName=" + SNserName + "&InstitutionsID=" + InstitutionsID + "&EducationalBackgroundID=" + EducationalBackgroundID);
        }
        $("#btnjiluSelect").click(function () {
            tabinreviewSearch();
        });

        //多条件查询
        function tabNoticeSearch() {
            var SUserName = $("#SUserName").val();
            var SeUserName = $("#SeUserName").val();
            var SeUserID = $("#SeUserID").val();
            if (SeUserID == undefined || SeUserID == "") {
                SeUserID = 0;
            }
            var StatusID = $("#StatusID").val();
            if (StatusID == undefined || StatusID == "") {
                StatusID = 0;
            }
            tabRechargeall.search("SUserName=" + SUserName + "&SeUserName=" + SeUserName + "&SeUserID=" + SeUserID + "&StatusID=" + StatusID);
        }

        $("#selectqueren").click(function () {
            tabNoticeSearch();
        });
        $("#btnSelect").click(function () {
            tabNoticeSearch();
        });
            //自定义公告详情列
            function crearViewButton(record, rowIndex, colIndex, options) {
                var shenghe = tabRechargeall.getColumnValue(rowIndex, "StatusName");
                if (shenghe=="待审核") {
                    return '<button type="button" style=display:compact; class="strShenHe btn btn-danger hidden" onclick="ShenHe(' + record.AuthenticationID + ')">审核</button>'
                 
                } else {
                    return '<button type="button" style=display:compact; class="strUpdata btn btn-info hidden" onclick="Updata(' + record.AuthenticationID + ')">修改</button>';
                }           
            }
            //学历认证绑定图片
            function Viewlogo(record, rowIndex, colIndex, options) {
                var AuthenticationID = tabRechargeall.getColumnValue(rowIndex, "AuthenticationID");
                $("#UimgPicture").attr("src", "/AuthenticationManagement/Authentication/GetImage");
                return '<img src="/AuthenticationManagement/Authentication/GetImage?AuthenticationID=' + AuthenticationID + '" alt="" width="60" height="30" onclick="myimage(' + record.AuthenticationID + ')"  id=' + AuthenticationID + ' />';
            }

            //认证记录绑定图片
            function PViewlogo(record, rowIndex, colIndex, options) {
                var AuthenticationID = tabinreview.getColumnValue(rowIndex, "AuthenticationID");
                $("#UnimgPicture").attr("src", "/AuthenticationManagement/Authentication/GetDiplomaImage");
                return '<img src="/AuthenticationManagement/Authentication/GetDiplomaImage?AuthenticationID=' + AuthenticationID + '" alt="" width="60" height="30"/>';

            }

            //放大图片
            function myimage(id) {
                $('#' + id).css({ "width": 700, "height": 500 });
                layer.open({
                    type: 1,
                    title: false,
                    closeBtn: 1,
                    area: '700px',
                    skin: 'layui-layer-nobg', //没有背景色
                    shadeClose: true,
                    content: $('#' + id),
                    end: function () {
                        $('#' + id).css({ "width": 60, "height": 30 });
                    }
                });
            }

            //审核
            function ShenHe(AuthenticationID) {
                $.post("/AuthenticationManagement/Authentication/Shenhebangdingxueli", { AuthenticationID: AuthenticationID }, function (data) {
                    loadDatatoForm("Updatashenhe", data);
                });
                $("#ValidateCode").attr("src", "/Fundsmanagement/management/validcode?ts=" + new Date().getTime());
                $("#ExamineRemarksID").val("");
                $("#validCode").val("");
                $("#modUpdatashenhe").modal("show");
            }

            //审核保存
            $("#updataShenhe").click(function () {
                if ($("input[name='optionsRadios'][type='radio']:checked").val() != undefined && $('#Updatashenhe [name="ExamineRemarks"]').val().trim() != "") {
                    var formDate = $("#Updatashenhe").serializeArray();
                    $.post("/AuthenticationManagement/Authentication/Updataxuelibaocun", formDate, function (msg) {
                        if (msg == "success") {
                            layer.alert('审核成功', { icon: 6, title: '提示' });
                            //关闭模态窗体
                            $("#modUpdatashenhe").modal("hide");
                            //刷新table
                            tabRechargeall.refreshPage();
                        }
                        else if (msg == "ValidCodeErro") {
                            $("#ValidateCode").attr("src", "/Fundsmanagement/management/validcode?ts=" + new Date().getTime());
                            layer.alert('验证码错误！', { icon: 5, title: '提示' });
                            $("#validCode").val("");
                        }
                        else {

                            layer.alert('审核失败！', { icon: 5, title: '提示' });
                        }
                    });
                }
                else {
                    layer.alert('审核状态和备注不能为空！', { icon: 2, title: '提示' });
                }
            });
            //修改
            function Updata(AuthenticationID) {            
                $.post("/AuthenticationManagement/Authentication/updataxuelibangding", { AuthenticationID: AuthenticationID }, function (data) {
                    loadDatatoForm("Updatashimingrenzheng", data);
                });
                //绑定图片
                $("#UimgStudentPicture").attr("src", "");
                $("#UimgStudentPicture").attr("src", "/AuthenticationManagement/Authentication/GetStudentImage?AuthenticationID=" + AuthenticationID);
                $("#Updatamodal").modal("show");
            }

            //编辑选择照片
            var imgReaderU = new FileReader();
            regexImageFilter = /^(?:image\/bmp|image\/png|image\/jpeg|image\/jpg)$/i;
            //文件读取 onload事件
            imgReaderU.onload = function (evt) {
                $("#UimgStudentPicture").attr("src", evt.target.result);
            }
            $("#UStudentPicture").change(function () {
                //选取选择图片
                var imgfFile = $("#UStudentPicture").prop('files')[0];
                //加载image标签中
                if (!regexImageFilter.test(imgfFile.type)) {
                    //alert("选择的不是一个有效的图片文件");
                    layer.msg('选择的不是一个有效的图片文件', { icon: 0 });
                }
                imgReaderU.readAsDataURL(imgfFile);
            });
            //修改保存按钮
            $("#QueRenupdata").click(function () {
                var StatusID = $('#Updatashimingrenzheng [name="StatusID"]').val();        
                var TrueName = $('#Updatashimingrenzheng [name="TrueName"]').val();
                var Institutions = $('#Updatashimingrenzheng [name="Institutions"]').val();
                var Specialty = $('#Updatashimingrenzheng [name="Specialty"]').val();
                var EducationalBackground = $('#Updatashimingrenzheng [name="EducationalBackground"]').val();
                var Intime = $('#Updatashimingrenzheng [name="Intime"]').val();
                var Entime = $('#Updatashimingrenzheng [name="Entime"]').val();
                var Diploma = $('#Updatashimingrenzheng [name="Diploma"]').prop('files');//照片
                if (StatusID > 0 && TrueName != '' && Institutions != '' && Specialty != '' && EducationalBackground != '' && Intime != '' && Entime != '' 
                   ) {
                    var layerIndex = layer.load(0);//显示 加载层
                    //提交表单
                    $("#Updatashimingrenzheng").ajaxSubmit(function (msg) {
                        layer.close(layerIndex);//关闭 加载层                 
                        if (msg == "success") {
                            layer.alert('修改成功', { icon: 1, title: '提示' });
                            tabRechargeall.refreshPage();
                            location.reload();
                            $("#Updatamodal").modal("hide");//关闭 modal 
                        } else if (msg == "exsit") {
                            layer.alert('用户名已存在！', { icon: 5, title: '提示' });                           
                         }else {
                            layer.alert('修改失败！', { icon: 6, title: '提示' });
                              }
                   });
                } else {
                    layer.alert('请填写完整', { icon: 0, title: '提示' });
                }
            })
        
    </script>
