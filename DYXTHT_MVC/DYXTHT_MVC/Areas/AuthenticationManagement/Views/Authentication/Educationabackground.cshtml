                                                <label class="col-lg-4 col-md-4 col-sm-4 control-label">专业：</label>
                                                <input type="text" class="form-control" id="SpecialtyID" name="Specialty">
                                            </div>
                                            <div class="form-group col-lg-6 col-md-6 col-sm-6">
                                                <label class="col-lg-4 col-md-4 col-sm-4 control-label">毕业时间：</label>                                              
                                                <input type="text" class="form-control" id="EntimeID" name="Entime">                                             
                                            </div>
