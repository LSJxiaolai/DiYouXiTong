                                        <div class="form-group col-lg-12 col-md-12 col-sm-12" style="margin-top:5px;">
                                            <div class="form-group col-lg-6 col-md-6 col-sm-6">
                                                <label class="col-lg-4 col-md-4 col-sm-4 control-label">学历：</label>
                                                <input type="text" class="form-control" id="EducationalBackground" name="EducationalBackground">
                                            </div>
                                            <div class="form-group col-lg-6 col-md-6 col-sm-6">
                                                <label class="col-lg-4 col-md-4 col-sm-4 control-label">审核状态：</label>                                              
                                                <select class="form-control" style="width:196px;" id="UpStatusID" name="StatusID"></select>                                             
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-12 col-md-12 col-sm-12" style="margin-top:5px;">
                                            <div class="form-group col-lg-6 col-md-6 col-sm-6">
                                                <label class="col-lg-4 col-md-4 col-sm-4 control-label">备注：</label>
                                                <textarea class="form-control" rows="4" name="ReviewRemarks" style="width:196px;" id="UpReviewRemarksID"></textarea>
                                            </div>                                          
                                            <div class="form-group col-lg-6 col-md-6 col-sm-6 img-box">
                                                <label class="col-lg-4 col-md-4 col-sm-4 control-label">学历照片:</label>
                                                <img src="" style="" alt="" width="200" height="100" id="UimgStudentPicture" />
                                                <input type="file" class="col-md-offset-4"  name="fileStudentImage" id="UStudentPicture" />
                                            </div>
                                       </div>
                                      
                                        <button type="button" class="btn btn-info col-md-offset-5" id="QueRenupdata" style="margin-top:30px;">确认修改</button>
                                    </form>
