                                            <label class="col-lg-5 col-md-5 col-sm-5 control-label">审核：</label>
                                            <label>
                                                <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1">
                                                审核
                                            </label>
                                            <label>
                                                <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2">
                                                审核通过
                                            </label>
                                            <label>
                                                <input type="radio" name="optionsRadios" id="optionsRadios3" value="option3">
                                                审核不通过
                                            </label>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-lg-5 col-md-5 col-sm-5 control-label">审核备注：</label>
                                            <div class="col-lg-4 col-md-4 col-sm-4">
                                                <textarea class="form-control" rows="3" name="ExamineRemarks" id="ExamineRemarksID"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="validCode" class="col-lg-5 col-md-5 col-sm-5 control-label">
                                                验证码:
                                            </label>
                                            <div class="col-lg-4 col-md-4 col-sm-4">
                                                <input type="text" class="form-control col-md-7" name="validCode" id="validCode" value="" style="width:132px;"/>
                                                <span class="text" style="padding:0"><img src="/Fundsmanagement/management/validcode" id="ValidateCode" onclick="reloadValideImage()" /></span>
                                            </div>
                                        </div>
                                        <button type="button" class="btn btn-info col-md-offset-5" id="updataShenhe" style="margin-top:30px;">确认审核</button>
                                    </form>
                                </div>
                            </div>
                        </div><!-- /.modal-content -->
                    </div><!-- /.modal-dialog -->
                </div>
                @*修改*@
                <div class="modal fade bs-example-modal-sm container" id="Updatamodal">
                    <div class="modal-dialog" style="width:800px;">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                                <h4 class="modal-title">修改学历认证</h4>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <form class="form-inline" role="form" id="Updatashimingrenzheng"  action="/AuthenticationManagement/Authentication/UpdateStudent" method="post">
