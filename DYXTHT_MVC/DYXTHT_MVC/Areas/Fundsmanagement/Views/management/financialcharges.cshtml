                var fromarray = $("#UPdataType").serializeArray();
                $.post("/Fundsmanagement/management/Updatazijinfeiyongtype", fromarray, function (data) {
                    if (data == "success") {
                        layer.alert('修改成功！', { icon: 1, title: '提示' });
