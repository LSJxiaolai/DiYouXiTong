    <script type="text/javascript">
      //修改保存
        $("#Submit").click(function () {
            layer.confirm("确定修改", { icon: 3, btn: ['确定', '取消'] }, function (layerIndex) {
                layer.close(layerIndex);
                var fordata = $("#UpdateForm").serializeArray();
                $.post("/Personal/Personal/UpdateMessage", fordata, function (data) {
                    if (data == "success") 
                    {
                        layer.alert("修改成功", { icon: 0, title: '提示' });
                        window.location.href = "/Main/BaseIndex";
                    }
                    else
                    {
                        layer.alert("修改失败", { icon: 0, title: '提示' });
                    }
                });
            });
      });
      $("#Back").click(function () {
          window.location.href = "/Main/BaseIndex";
      });
