       //我的账户
       $("#MyAccountID").click(function () {
           $.post("/Main/sesssinoselect", function (data) {
               if (data == "success") {
                   window.location.href = "/Personal/Personal/BaseIndex";
               }
               else {
                   layer.confirm("请先登录？", {
                       icon: 3,
                       btn: ['确定', '取消']
                       //按钮
                   }, function (layerIndex) {
                       var layerIndex = layer.load(0);//显示 加载层
                       window.location.href = "/Main/Login";
                   });
               }
           });
       });
       //我的投资
       $("#myInvest").click(function () {
