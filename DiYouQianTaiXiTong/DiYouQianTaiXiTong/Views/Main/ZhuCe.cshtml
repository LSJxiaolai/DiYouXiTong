    <script type="text/javascript">
        //注册
        $("#reg_sub").click(function () {
            var fordata = $("#reg_form").serializeArray();
            $.post("/Main/InsertYongHu", fordata, function (data) {
                if (data == "Exist")
                {
                    layer.alert("该用户已存在", { icon: 0, title: '提示' }, function (layerIndex) {
                        layer.close(layerIndex);
                    });
                } else if (data == "success") {
                    layer.alert("注册成功！请登录！", { icon: 1, title: '提示' }, function (layerIndex) {
                        layer.close(layerIndex);
                        window.location.href = "/Main/Login";
                    });
                }
                else {
                    layer.alert("注册失败", { icon: 2, title: '提示' });
                }
            });
        });
        //验证码刷新方法
        function reloadValideImage() {
            document.getElementById("ValidateCode").src = '/Main/ValideCode?ts=' + new Date().getTime();
        }
    </script>

