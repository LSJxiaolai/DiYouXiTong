    <script type="text/javascript">
        //刷新验证码
        function reloadValideImage() {
            document.getElementById("ValidateCode").src = '/Main/ValideCode?ts=' + new Date().getTime();
        };
        //提交
        $("#Submit").click(function () {
            layer.confirm("确定要撤标吗", {
                icon: 3,
                btn: ['确定', '取消']
            }, function (layerIndex) {
                layer.close(layerIndex);
                var fordata = $("#CheLoanForm").serializeArray();
                $.post("/Personal/Personal/RevokeLoan", fordata, function (data) {
                    if (data == "success") {
                        alert("撤销成功！");
                        location.reload();
                        window.location.href = "/Main/LoanManageIndex";
                    } else if (data == "validCodeFail") {
                        layer("验证码错误");
                        $("#validCode").val("");
                        reloadValideImage();
                    } else {
                        alert("撤标失败!");
                    }
                });
            });
        });
        //取消
        $("#Return").click(function () {
            $("#reloadValideImage").val("");
            window.location.href = "/Main/LoanManageIndex";
            //$("#CheLoanInfor").modal('hide');
        });
    </script>

