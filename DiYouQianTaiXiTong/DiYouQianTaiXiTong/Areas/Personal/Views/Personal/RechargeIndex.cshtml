                pageSizeSelect: true,//是否选择分页页数下拉框
                pagingLittleToolbar: true,//精简的图标按钮分页工具条
                pagingToolbarAlign: "left",//分页工具条的显示位置
            });
            tabCashRecordSearch();
        });
        //充值记录多条件查询
        function tabRechangeRecordSearch() {
            var payTypeID = $("#TPayTypeName").val();
            var rechargeTypeID = $("#TRechargeTypeName").val();
            if (payTypeID == undefined || payTypeID == "") {
                payTypeID = 0;
            }
            if (rechargeTypeID == undefined || rechargeTypeID == "")
            {
                rechargeTypeID = 0;
            }
            tabRechangeRecord.search("payTypeID=" + payTypeID + "&rechargeTypeID=" + rechargeTypeID);
        }

        //通过选择省份选择城市
        $("#ProID").click(function () {
            var proID = $("#ProID").val();
            $.post("/Personal/Personal/SelectCashCity", { proID: proID }, function (data) {
                selectLoadData("CityID", data);
            });
        });
        //查询
        $("#btnSelect").click(function () {
            tabRechangeRecordSearch();
        });
        //实际到账的金额、充值后的账户可用余额的计算
        $("#RechargeMoney").change(function () {
            var RechargeMoney = $("#RechargeMoney").val();
            var NowMoney = parseFloat($("#NowMoney").val());
            var yy = RechargeMoney * 0.001;
            var RechargePoundage = $("#RechargePoundage").val(yy);
            var RealityAccountMoney = parseFloat($("#RealityAccountMoney").val(RechargeMoney - yy));
            var realityAccountMoney =parseFloat($("#RealityAccountMoney").val());
            if (NowMoney == NaN) {
                $("#FinalMoney").val(NowMoney + realityAccountMoney);
            }
            else {
                parseFloat($("#FinalMoney").val(NowMoney+realityAccountMoney));

            }
        });
        //充值新增
        $("#Submit").click(function () {
            var fordata = $("#Infor").serializeArray();
            $.post("/Personal/Personal/InsertRecharge", fordata, function (data) {
                if (data == "success") {
                    layer.alert("充值成功,等待审核", { icon: 1, title: '提示' }, function (layerIndex) {
                        layer.close(layerIndex);
                        location.reload();
                    });
                } else
                {
                    layer.alert("充值失败", { icon: 2, title: '提示' });
                }
            });
        })


        //计算提现到账金额
        $("#CashAmount").change(function () {
            var CashMoney = $("#CashAmount").val();
            var CashPoundage = CashMoney * 0.001;
            $("#Poundage").val(CashPoundage);
            $("#AccountMoney").val(CashMoney - CashPoundage);
        });

        //提现多条件查询
        function tabCashRecordSearch() {
            var payTypeID = $("#CPayTypeName").val();
            if (payTypeID == undefined || payTypeID == "")
            {
                payTypeID = 0;
            }
            tabCashRecord.search("payTypeID=" + payTypeID);
        }

        $("#CbtnSelect").click(function () {
            tabCashRecordSearch();
        });
        //提现新增
        $("#CSubmit").click(function () {
            var fordata = $("#CashInfor").serializeArray();
            $.post("/Personal/Personal/InsertCash", fordata, function (data) {
                if (data == "success")
                {
                    layer.alert("提交成功,等待审核", { icon: 1, title: '提示' }, function (layerIndex) {
                        layer.close(layerIndex);
                        location.reload();
                    });
                } else if (data == "fail") {
                    layer.alert("提现失败", { icon: 2, title: '提示' }, function (layerIndex) {
                        layer.close(layerIndex);
                        location.reload();
                    });
                } else if (data == "lose") {
                    layer.alert("账号格式不正确", { icon: 0, title: '提示' }, function (layerIndex) {
                        layer.close(layerIndex);
                        $("#CashAccountNumber").val("");
                    });
                } else {
                    layer.alert("账户余额不足", { icon:3, title: '提示' }, function (layerIndex) {
                        layer.close(layerIndex);
                        $("#CashAmount").val("");

                    });
                }
            });
        });

        //重新加载Form表单文本框的内容
        $("#ChongZhi").click(function () {
            location.reload();
        });

    </script>
</body>
</html>

