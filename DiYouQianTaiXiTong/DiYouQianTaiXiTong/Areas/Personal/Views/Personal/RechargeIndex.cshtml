    <script type="text/javascript">
        $(document).ready(function () {
            $.post("/Main/sesssinoselect", function (data) {
                if (data == "success") {
                    $("div").remove("#yuanlaiID");
                }
                else {
                    $("div").remove("#userviweID");
                }
            });
        });
        var tabRechangeRecord = 0;
        var tabCashRecord = 0;
        $(document).ready(function () {
           // 充值方式下拉框
            $.post("/Personal/Personal/RechargeType", function (data) {
                selectLoadData("RechargeTypeName", data);
            });
            //充值记录中充值方式下拉框
            $.post("/Personal/Personal/RechargeType", function (data) {
                selectLoadData("TRechargeTypeName", data);
            });
            //充值类型下拉框
            $.post("/Personal/Personal/PayType", function (data) {
                selectLoadData("PayTypeName", data);
            });
            //充值记录中充值类型下拉框
            $.post("/Personal/Personal/PayType", function (data) {
                selectLoadData("TPayTypeName", data);
            });

           //绑定账户原有的金额
            $.post("/Personal/Personal/SelectMoney", function (data) {
                $("#NowMoney").val(data.PropertyAmounts);
            });


            //绑定充值tab
            tabRechangeRecord = $.fn.bsgrid.init('tabRechangeRecord', {
                url: '/Personal/Personal/SelectRechargeRcord',
                autoLoad: true,
                stripeRows: true,//隔行变色
                rowHoverColor: true,//划过行变色
                rowSelectedColor: true,//是否显示选中行背景色
                pageSize: 10,//默认页面大小
                pageSizeSelect: true,//是否选择分页页数下拉框
                pagingLittleToolbar: true,//精简的图标按钮分页工具条
                pagingToolbarAlign: "left",//分页工具条的显示位置
            });
            tabRechangeRecordSearch();



            //绑定用户真实姓名
            $.post("/Personal/Personal/SelectTrueName", function (data) {
                $("#TrueName").val(data.TrueName);
            });
